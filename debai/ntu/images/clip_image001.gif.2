    <script type="text/javascript">
        $(function () {
            //jQuery.ajaxSetup({ cache: false });

            $('.countdown').each(function (i, obj) {
                var h = $(this);
                var d = new Date(this.id);
                h.countdown({
                    until: d,
                    format: 'HMS',
                    padZeroes: true,
                    compact: true,
                    serverSync: serverTime
                });
            });
        });

        function serverTime() {
            var time = null;
            var d = new Date();
            var n = d.getTime();
            $.ajax({
                url: '/Contest/ServerTime' + '/?time=' + n,
                async: false, dataType: 'text',
                success: function (text) {
                    time = new Date(text);
                }, error: function (http, message, exc) {
                    time = new Date();
                }
            });
            return time;
        }

        var dialog;
        var wid = 960;
        if ($(window).width() <= 720) {
            wid = $(window).width();
        }
        dialog = $("#dialog-form").dialog({
            autoOpen: false,
            height: 600,
            width: wid,
            modal: true,
            open: function () {
                jQuery('.ui-widget-overlay').bind('click', function () {
                    dialog.dialog('close');
                })
            }
        });

        $(".submission-status").on("click", function () {
            dialog.dialog("open");
            $("#dialog-form").html('<span class="checking">loading...</span>');
            $("#dialog-form").load('/Submission/Detail/' + this.id);
        });

        $(".contest-takepart").on("click", function () {
            dialog.dialog("open");
            $("#dialog-form").html('<span class="checking">loading...</span>');
            $("#dialog-form").load('/Contest/TakePart/' + this.id, function (response, status, xhr) {
                if (status != "error") {
                    $(".submission-status").on("click", function () {
                        dialog.dialog("open");
                        $("#dialog-form").load('/Submission/Detail/' + this.id);
                     });
                }
            });
        });

        var amountScrolled = 300;

        $(window).scroll(function () {
            if ($(window).scrollTop() > amountScrolled) {
                $('a.back-to-top').fadeIn('slow');
            } else {
                $('a.back-to-top').fadeOut('slow');
            }
        });

        $('a.back-to-top').click(function () {
            $('html,body').animate({
                scrollTop: 0
            }, 700);
            return false;
        });

    </script>

    
